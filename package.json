{
  "name": "webpack4-mpa",
  "version": "1.0.0",
  "description": "多页架构！大型网站架构！并支持ssr！This is super recommened!!!",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "start": "cross-env NODE_ENV=production node start.js",
    "build": "rimraf dist && npm run build:client && npm run build:server && npm run build:client-ssr",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.conf.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.conf.js",
    "build:client-ssr": "cross-env NODE_ENV=production webpack --config ./build/webpack.ssr.client.conf.js",
    "postinstall": "npm run build"
  },
  "author": "fmx",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@vue/compiler-sfc": "^3.1.2",
    "@vue/server-renderer": "^3.1.2",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "event-source-polyfill": "^1.0.24",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^4.14.1",
    "postcss": "^6.0.23",
    "postcss-loader": "^4.3.0",
    "postcss-px-to-viewport": "^1.1.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-bundle-renderer": "0.0.3",
    "vue-loader": "^16.3.0",
    "vue-server-renderer": "^2.6.14",
    "vue-style-loader": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "lru-cache": "^4.1.5",
    "route-cache": "^0.4.5",
    "serialize-javascript": "^6.0.0",
    "vue": "^3.1.2",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2",
    "vuex-router-sync": "^5.0.0"
  }
}
